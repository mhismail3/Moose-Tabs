{
  "manifest_version": 3,
  "name": "__MSG_app_title__",
  "version": "1.0.0",
  "description": "__MSG_extension_description__",
  "default_locale": "en",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "sidePanel",
    "storage",
    "contextMenus"
  ],
  "side_panel": {
    "default_path": "index.html"
  },
  "action": {
    "default_title": "__MSG_action_title__"
  },
  "web_accessible_resources": [
    {
      "resources": ["settings.html", "settings-bundle.js", "settings-loading.css", "settings-init.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}