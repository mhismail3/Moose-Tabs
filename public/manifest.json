{
  "manifest_version": 3,
  "name": "Moose-Tabs",
  "version": "1.0.0",
  "description": "Chrome extension that tracks tab hierarchies and parent-child relationships",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "index.html"
  },
  "action": {
    "default_title": "Open Moose-Tabs"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}